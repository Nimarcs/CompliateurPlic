    ecrire k = 2 ;
    ecrire k = 5 ;
    ecrire 910 ;
    //>
    ecrire k > 2 ;
    ecrire k > 5 ;
    ecrire k > 1 ;
    ecrire 9001 ;
    //<
    ecrire k < 2 ;
    ecrire k < 5 ;
    ecrire k < 1 ;
