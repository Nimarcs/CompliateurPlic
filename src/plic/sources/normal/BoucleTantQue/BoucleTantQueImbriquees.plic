programme plic {
    entier x ;
    entier i ;
    entier j ;
    x := 0 ;
    i := 2 ;
    j := 2 ;
    tantque ( i >= 0 ) repeter {
        tantque ( j >= 0 ) repeter {
            ecrire ( x ) ;
            j := j - 1 ;
